{
    "manifest_version": 2,
 
    "name": "PassGen",
    "description": "This extension generates passwords",
    "version": "1.0",
 
    
    "background": {
        "scripts": ["background.js"],
        "persisent": false
    },

    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["content.js"]
        }
    ],

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },


    "permissions": [
        "activeTab",
        "http://192.168.1.95:3000/*"
    ]
}